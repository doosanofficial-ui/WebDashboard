{
  "name": "telemetry-mobile",
  "version": "0.1.0",
  "private": true,
  "description": "Bare React Native telemetry client scaffold",
  "main": "index.js",
  "scripts": {
    "init-native": "bash ./scripts/init-native-project.sh",
    "ios:setup-bg": "bash ./scripts/apply-ios-location-background.sh",
    "android:setup-bg": "bash ./scripts/apply-android-location-background.sh",
    "start": "react-native start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "validate": "node ./scripts/validate-mobile-scaffold.js",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/geolocation": "^3.4.0",
    "react": "18.3.1",
    "react-native": "0.76.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.29.0",
    "@react-native-community/cli": "^15.1.3",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "@react-native-async-storage/async-storage": "<rootDir>/tests/__mocks__/async-storage.js"
    }
  }
}
